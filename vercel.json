{
  "version": 2,
  "builds": [
    {
      "src": "unified_parser.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    },
    {
      "src": "templates/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "unified_parser.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "regions": ["iad1"]
}
